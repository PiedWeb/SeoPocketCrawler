{
  "name": "piedweb/seo-pocket-crawler",
  "type": "library",
  "description": "Web Crawler to check few SEO basics.",
  "keywords": [
    "Pied Web",
    "SeoPocketCrawler"
  ],
  "homepage": "https://dev.piedweb.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin D. (ak Pied Web)",
      "email": "contact@robin-d.fr",
      "homepage": "https://piedweb.com"
    }
  ],
  "require": {
    "php": "~7.3|^8.0",
    "piedweb/url-harvester": "^0.0.27",
    "league/uri": "dev-master",
    "league/csv": "^9.1",
    "piedweb/curl": "^0.0",
    "symfony/console": "^5.0",
    "vimeo/psalm": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "PiedWeb\\SeoPocketCrawler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PiedWeb\\SeoPocketCrawler\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  }
}
